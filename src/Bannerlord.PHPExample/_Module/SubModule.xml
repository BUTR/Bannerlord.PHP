<?xml version="1.0" encoding="UTF-8"?>
<Module xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/BUTR/Bannerlord.XmlSchemas/master/SubModule.xsd" >
  <Id value="$moduleid$" />
  <Name value="$modulename$" />
  <Version value="v$version$" />
  <DefaultModule value="false" />
  <ModuleType value="Community" />
  <ModuleCategory value="Singleplayer" />
  <Url value="" />
  <DependedModules>
    <DependedModule Id="Bannerlord.PHP" DependentVersion="v$phpversion$" />

    <DependedModule Id="Native" DependentVersion="v$gameversion$.*" />
    <DependedModule Id="SandBoxCore" DependentVersion="v$gameversion$.*" />
    <DependedModule Id="Sandbox" DependentVersion="v$gameversion$.*" />
    <DependedModule Id="StoryMode" DependentVersion="v$gameversion$.*" />
    <DependedModule Id="CustomBattle" DependentVersion="v$gameversion$.*" />
  </DependedModules>
  <!-- Community Dependency Metadata -->
  <!-- https://github.com/BUTR/Bannerlord.BUTRLoader#for-modders -->
  <DependedModuleMetadatas>
    <DependedModuleMetadata id="Bannerlord.PHP" order="LoadBeforeThis" version="v$phpversion$" />

    <DependedModuleMetadata id="Native" order="LoadBeforeThis" optional="true" version="v$gameversion$.*" />
    <DependedModuleMetadata id="SandBoxCore" order="LoadBeforeThis" optional="true" version="v$gameversion$.*" />
    <DependedModuleMetadata id="Sandbox" order="LoadBeforeThis" optional="true" version="v$gameversion$.*" />
    <DependedModuleMetadata id="StoryMode" order="LoadBeforeThis" optional="true" version="v$gameversion$.*" />
    <DependedModuleMetadata id="CustomBattle" order="LoadBeforeThis" optional="true" version="v$gameversion$.*" />
  </DependedModuleMetadatas>
  <!-- Community Dependency Metadata -->
  <SubModules>
    <SubModule>
      <Name value="$moduleid$" />
      <DLLName value="$moduleid$.dll" />
      <SubModuleClassType value="$moduleid$.SubModule" />
      <Tags/>
    </SubModule>
  </SubModules>
</Module>